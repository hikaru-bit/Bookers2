<h1>User Info</h1>
<%= form_with model:@user, local: true do |f| %>
  <h4>Name</h4>
  <%= f.text_field :name %>

  <%= render 'profile_images/new',locals:{ profile_images: @profile_images} %>
  <h4>Introduction</h4>
  <%= f.text_field :introduction %><br>
  <%= f.submit 'Update User' %><br>

<% end %>